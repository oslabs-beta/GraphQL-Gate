{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "airbnb-base"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
        "indent": [
            "warn",
            2,
            {
                "SwitchCase": 1
            }
        ]
    }
}
